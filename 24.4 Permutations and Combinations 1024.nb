 RowBox[{
  RowBox[{"stir", "[", "n_", "]"}], "=", 
  RowBox[{
   RowBox[{"Sqrt", "[", 
    RowBox[{"2", " ", "\[Pi]", " ", "n"}], "]"}], 
   SuperscriptBox[
    RowBox[{"(", 
     FractionBox["n", "\[ExponentialE]"], ")"}], "n"]}]}]], "Input",
 CellChangeTimes->{{3.766068432664308*^9, 3.76606847508445*^9}}],


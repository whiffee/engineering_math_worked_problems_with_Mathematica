 RowBox[{
  SuperscriptBox["\[ExponentialE]", 
   RowBox[{"-", "n"}]], " ", 
  SuperscriptBox["n", 
   RowBox[{
    FractionBox["1", "2"], "+", "n"}]], " ", 
  SqrtBox[
   RowBox[{"2", " ", "\[Pi]"}]]}]], "Output",
 CellChangeTimes->{3.7660684757741737`*^9}]
}, Open  ]],


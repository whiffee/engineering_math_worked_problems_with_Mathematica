 CellChangeTimes->{3.768603953805214*^9, 3.768604633601308*^9, 
  3.768655810695833*^9, 3.769696294211833*^9, 3.7697047064968843`*^9}]
}, Open  ]],

Cell[TextData[{
 "Note above that the largest minus exponent in \[OpenCurlyQuote]y2prime\
\[CloseCurlyQuote] is ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", 
    RowBox[{"-", "2"}]], TraditionalForm]]],
 ". The differential equation itself is the next to be created. Because of \
the limited presence of minus exponents on x, multiplying through by ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "2"], TraditionalForm]]],
 " here normalizes the series so that the lowest order term appearing will be \
",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "r"], TraditionalForm]]],
 "."
}], "Text",
 CellChangeTimes->{
  3.768603971152169*^9, {3.773928278321149*^9, 3.773928281183303*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    SuperscriptBox["x", "2"], "*", 
    RowBox[{"(", 
     RowBox[{"y2prime", "+", 
      RowBox[{"pseries", "*", "yprime"}], "+", 
      RowBox[{"qseries", "*", "y"}]}], ")"}]}], "//", "Expand"}], 
  ";"}]], "Input",
 CellChangeTimes->{3.768604002491232*^9}],


}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"minroot", "=", 
  RowBox[{"Min", "[", 
   RowBox[{"r", "/.", "indroots"}], "]"}]}]], "Input"],


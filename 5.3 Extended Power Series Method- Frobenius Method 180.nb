Cell[TextData[{
 "Note above that ",
 StyleBox["Collect", "Input"],
 " has ordered the terms so that the ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "r"], TraditionalForm]]],
 " term comes first. This may not always be the case, and if it is not, it \
will affect the following steps. A necessary preliminary step to finding the \
indicial roots is"
}], "Text",
 CellChangeTimes->{3.7686040815373793`*^9}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"firstcoeff", "=", 
  RowBox[{"lhs", "[", 
   RowBox[{"[", 
    RowBox[{"1", ",", "2"}], "]"}], "]"}]}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{"-", 
   FractionBox[
    RowBox[{"c", "[", "0", "]"}], "2"]}], "+", 
  RowBox[{
   FractionBox["1", "2"], " ", "r", " ", 
   RowBox[{"c", "[", "0", "]"}]}], "+", 
  RowBox[{
   SuperscriptBox["r", "2"], " ", 
   RowBox[{"c", "[", "0", "]"}]}]}]], "Output",
 CellChangeTimes->{3.768604109911394*^9, 3.7686046481370707`*^9, 
  3.768655822400544*^9}]
}, Open  ]],

Cell["\<\
Note two things about firstcoeff above. It contains elements of r, and it \
contains a single constant parameter, here c[0]. Both of these \
characteristics are needed to make the example work. To reveal the roots, set \
the above expression to zero and solve as follows.\
\>", "Text",
 CellChangeTimes->{{3.768604121861083*^9, 3.768604138329701*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"indroots", "=", 
   RowBox[{"Solve", "[", 
    RowBox[{
     RowBox[{"firstcoeff", "\[Equal]", "0"}], ",", "r"}], "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{3.768604176328367*^9}],

Cell["Order the roots by size", "Text",
 CellChangeTimes->{3.768604191040534*^9}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"maxroot", "=", 
  RowBox[{"Max", "[", 
   RowBox[{"r", "/.", "indroots"}], "]"}]}]], "Input"],

Cell[BoxData[
 FractionBox["1", "2"]], "Output",
 CellChangeTimes->{3.768604208012847*^9, 3.768604654404812*^9, 
  3.768655829572139*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"minroot", "=", 
  RowBox[{"Min", "[", 
   RowBox[{"r", "/.", "indroots"}], "]"}]}]], "Input"],

Cell[BoxData[
 RowBox[{"-", "1"}]], "Output",
 CellChangeTimes->{3.768604225691931*^9, 3.7686046571509647`*^9, 
  3.768655832544826*^9}]
}, Open  ]],

Cell[TextData[Cell[BoxData[
 StyleBox[
  RowBox[{
   RowBox[{
   "The", " ", "next", " ", "step", " ", "is", " ", "the", " ", "first", " ", 
    "in", " ", "a", " ", "series", " ", "of", " ", "six", " ", "steps", " ", 
    "to", " ", "find", " ", "the", " ", "first", " ", "solution"}], ",", " ", 
   
   RowBox[{
    FormBox[
     SubscriptBox["y", "1"],
     TraditionalForm], "."}]}],
  FontFamily->"Charter"]], "Text",

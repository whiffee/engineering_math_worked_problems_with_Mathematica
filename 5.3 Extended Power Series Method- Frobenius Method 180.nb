   RowBox[{"(", 
    RowBox[{"4", "+", "r"}], ")"}], " ", 
   SuperscriptBox["x", 
    RowBox[{"2", "+", "r"}]], " ", 
   RowBox[{"c", "[", "4", "]"}]}], "+", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"4", "+", "r"}], ")"}], " ", 
   RowBox[{"(", 
    RowBox[{"5", "+", "r"}], ")"}], " ", 
   SuperscriptBox["x", 
    RowBox[{"3", "+", "r"}]], " ", 
   RowBox[{"c", "[", "5", "]"}]}], "+", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"5", "+", "r"}], ")"}], " ", 
   RowBox[{"(", 
    RowBox[{"6", "+", "r"}], ")"}], " ", 
   SuperscriptBox["x", 
    RowBox[{"4", "+", "r"}]], " ", 

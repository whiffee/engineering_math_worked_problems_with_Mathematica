Cell[TextData[{
 "One obvious advantage of the Bessel version is that it is not necessary to \
particularize it in order to get a plot. At the site ",
 StyleBox["https://mathematica.stackexchange.com/questions/183164/\
ndeigenvalues-vs-findroot-for-finding-the-eigenvalues-of-airy-equation/183276#\
183276",
  FontSize->14,
  FontSlant->"Italic"],
 ", in the answer of Bill Watts, are some functions that convert Airy to \
Bessel. P is for positive x, and M is for minus x."
}], "Text",
 CellChangeTimes->{{3.7698813603235407`*^9, 3.769881398688285*^9}, {
  3.769881487220792*^9, 3.769881538769389*^9}, {3.769881747047783*^9, 
  3.769881750390236*^9}, {3.769883405985115*^9, 3.769883406622497*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"AiryToBesP", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"AiryAi", "[", "x_", "]"}], "\[Rule]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "/", "3"}], ")"}], "*", 
       RowBox[{"Sqrt", "[", "x", "]"}], "*", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"BesselI", "[", 
          RowBox[{
           RowBox[{"-", 
            RowBox[{"3", "^", 
             RowBox[{"(", 
              RowBox[{"-", "1"}], ")"}]}]}], ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"2", "/", "3"}], ")"}], "*", 
            RowBox[{"x", "^", 
             RowBox[{"(", 
              RowBox[{"3", "/", "2"}], ")"}]}]}]}], "]"}], "-", 
         RowBox[{"BesselI", "[", 
          RowBox[{
           RowBox[{"1", "/", "3"}], ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"2", "/", "3"}], ")"}], "*", 
            RowBox[{"x", "^", 
             RowBox[{"(", 
              RowBox[{"3", "/", "2"}], ")"}]}]}]}], "]"}]}], ")"}]}]}], ",", 
     RowBox[{
      RowBox[{"AiryBi", "[", "x_", "]"}], "\[Rule]", 
      RowBox[{
       RowBox[{"Sqrt", "[", 
        RowBox[{"x", "/", "3"}], "]"}], "*", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"BesselI", "[", 
          RowBox[{
           RowBox[{"-", 
            RowBox[{"3", "^", 
             RowBox[{"(", 
              RowBox[{"-", "1"}], ")"}]}]}], ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"2", "/", "3"}], ")"}], "*", 
            RowBox[{"x", "^", 
             RowBox[{"(", 
              RowBox[{"3", "/", "2"}], ")"}]}]}]}], "]"}], "+", 
         RowBox[{"BesselI", "[", 
          RowBox[{
           RowBox[{"1", "/", "3"}], ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"2", "/", "3"}], ")"}], "*", 
            RowBox[{"x", "^", 
             RowBox[{"(", 
              RowBox[{"3", "/", "2"}], ")"}]}]}]}], "]"}]}], ")"}]}]}]}], 
    "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"AiryToBesM", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"AiryAi", "[", "x_", "]"}], "\[Rule]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "/", "3"}], ")"}], "*", 
       RowBox[{"Sqrt", "[", 
        RowBox[{"-", "x"}], "]"}], "*", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"BesselJ", "[", 
          RowBox[{
           RowBox[{"-", 
            RowBox[{"3", "^", 
             RowBox[{"(", 
              RowBox[{"-", "1"}], ")"}]}]}], ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"2", "/", "3"}], ")"}], "*", 
            RowBox[{
             RowBox[{"Sqrt", "[", 
              RowBox[{"-", "x"}], "]"}], "^", "3"}]}]}], "]"}], "+", 
         RowBox[{"BesselJ", "[", 
          RowBox[{
           RowBox[{"1", "/", "3"}], ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"2", "/", "3"}], ")"}], "*", 
            RowBox[{
             RowBox[{"Sqrt", "[", 
              RowBox[{"-", "x"}], "]"}], "^", "3"}]}]}], "]"}]}], ")"}]}]}], 
     ",", 
     RowBox[{
      RowBox[{"AiryBi", "[", "x_", "]"}], "\[Rule]", 
      RowBox[{
       RowBox[{"Sqrt", "[", 
        RowBox[{
         RowBox[{"-", "x"}], "/", "3"}], "]"}], "*", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"BesselJ", "[", 
          RowBox[{
           RowBox[{"-", 
            RowBox[{"3", "^", 
             RowBox[{"(", 
              RowBox[{"-", "1"}], ")"}]}]}], ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"2", "/", "3"}], ")"}], "*", 
            RowBox[{
             RowBox[{"Sqrt", "[", 
              RowBox[{"-", "x"}], "]"}], "^", "3"}]}]}], "]"}], "-", 
         RowBox[{"BesselJ", "[", 
          RowBox[{
           RowBox[{"1", "/", "3"}], ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"2", "/", "3"}], ")"}], "*", 
            RowBox[{
             RowBox[{"Sqrt", "[", 
              RowBox[{"-", "x"}], "]"}], "^", "3"}]}]}], "]"}]}], ")"}]}]}]}],
     "}"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.76988135055313*^9, 3.7698813505649137`*^9}, {
  3.769881764056343*^9, 3.76988177624575*^9}}],

Cell["Carrying on with the above choice of positive x,", "Text",
 CellChangeTimes->{{3.769882663367281*^9, 3.769882687134465*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"y", "[", "x_", "]"}], "=", 
  RowBox[{
   RowBox[{
    RowBox[{"solN", "[", "x", "]"}], "/.", "AiryToBesP"}], "//", 

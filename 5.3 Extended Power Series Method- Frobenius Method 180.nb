    RowBox[{"1", "+", "r"}], ")"}], " ", 
   RowBox[{"(", 
    RowBox[{"2", "+", "r"}], ")"}], " ", 
   SuperscriptBox["x", "r"], " ", 
   RowBox[{"c", "[", "2", "]"}]}], "+", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"2", "+", "r"}], ")"}], " ", 
   RowBox[{"(", 
    RowBox[{"3", "+", "r"}], ")"}], " ", 
   SuperscriptBox["x", 
    RowBox[{"1", "+", "r"}]], " ", 
   RowBox[{"c", "[", "3", "]"}]}], "+", 

Cell[TextData[{
 "11.  Find the probability that none of three bulbs in a traffic signal will \
have to be replaced during the first 1500 hours of operation if the lifetime \
X of a bulb is a random variable with the density f[x] = 6 (0.25 - (x - 1.5",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[")", "2"], TraditionalForm]]],
 ") when 1 \[LessEqual] x \[LessEqual] 2 and f[x] = 0 otherwise, where x is \
measured in multiples of 1000 hours."
}], "Text",
 CellChangeTimes->{{3.766080398190427*^9, 3.766080528479151*^9}},
 Background->RGBColor[1, 0.9, 0.8]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"f", "[", "x_", "]"}], "=", 
  RowBox[{"Piecewise", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"0", ",", 
       RowBox[{
        RowBox[{"-", "\[Infinity]"}], "<", "x", "<", "1"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"6", " ", 
        RowBox[{"(", 
         RowBox[{"0.25", "-", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"x", "-", "1.5"}], ")"}], "2"]}], ")"}]}], ",", 
       RowBox[{"1", "\[LessEqual]", "x", "\[LessEqual]", "2"}]}], "}"}], ",", 
     
     RowBox[{"{", 
      RowBox[{"0", ",", 
       RowBox[{"2", "<", "x", "<", "\[Infinity]"}]}], "}"}]}], "}"}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.766099301956189*^9, 3.766099344809411*^9}, 

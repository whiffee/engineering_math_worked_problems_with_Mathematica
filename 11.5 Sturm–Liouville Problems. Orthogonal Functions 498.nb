Cell[TextData[{
 "The cells below show how to run a DSolve function after dropping boundary \
values, then employing the ",
 StyleBox["Reduce", "Input"],
 " command to force the boundary values to reveal the eigenvalues. However, \
in this case it does not work.  However, it did work on the Stack Exchange \
page where I saw it. (search for eigenfunction)."
}], "Text",
 CellChangeTimes->{{3.754348491466714*^9, 3.754348633356966*^9}, {
  3.754348667413342*^9, 3.754348678379561*^9}, {3.754348916139526*^9, 
  3.754348920267908*^9}, {3.754349114264048*^9, 3.754349133015729*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"Reduce", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"y", "'"}], "[", "0", "]"}], "\[Equal]", "0"}], "&&", 
      RowBox[{
       RowBox[{
        RowBox[{"y", "'"}], "[", "1", "]"}], "\[Equal]", "0"}], "&&", 
      RowBox[{"a", "\[NotEqual]", "0"}], "&&", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"C", "[", "1", "]"}], "\[NotEqual]", "0"}], "||", 
        RowBox[{
         RowBox[{"C", "[", "2", "]"}], "\[NotEqual]", "0"}]}], ")"}]}], "/.", 
     
     RowBox[{"DSolve", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"y", "''"}], "[", "x", "]"}], "+", 
         RowBox[{
          RowBox[{"a", "^", "2"}], " ", 
          RowBox[{"y", "[", "x", "]"}]}]}], "\[Equal]", "0"}], ",", "y", ",", 
       "x"}], "]"}]}], ",", "a"}], "]"}], "//", "FullSimplify"}]], "Input",
 CellChangeTimes->{{3.754336391764553*^9, 3.754336391772315*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"C", "[", "3", "]"}], "\[Element]", "Integers"}], "&&", 
  RowBox[{
   RowBox[{"C", "[", "2", "]"}], "\[Equal]", "0"}], "&&", 
  RowBox[{
   RowBox[{"C", "[", "1", "]"}], "\[NotEqual]", "0"}], "&&", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"a", "\[Equal]", 
       RowBox[{"2", " ", "\[Pi]", " ", 
        RowBox[{"C", "[", "3", "]"}]}]}], "&&", 
      RowBox[{"a", "\[NotEqual]", "0"}]}], ")"}], "||", 
    RowBox[{
     RowBox[{"\[Pi]", "+", 
      RowBox[{"2", " ", "\[Pi]", " ", 
       RowBox[{"C", "[", "3", "]"}]}]}], "\[Equal]", "a"}]}], 
   ")"}]}]], "Output",
 CellChangeTimes->{3.754336397109071*^9, 3.754348335617957*^9, 
  3.77315403301828*^9}]
}, Open  ]],


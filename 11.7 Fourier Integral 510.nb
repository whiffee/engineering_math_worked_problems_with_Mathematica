     RowBox[{"\[Pi]", " ", "w"}]], 
    RowBox[{"Sin", "[", 
     RowBox[{"w", " ", "x"}], "]"}], 
    RowBox[{"\[DifferentialD]", "w"}]}]}]}]], "Input",

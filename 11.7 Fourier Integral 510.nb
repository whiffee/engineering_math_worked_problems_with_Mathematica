    RowBox[{"Cos", "[", 
     RowBox[{"w", " ", "v"}], "]"}], 
    RowBox[{"\[DifferentialD]", "v"}]}]}]}]], "Input",
 CellChangeTimes->{{3.75451940721094*^9, 3.754519472204946*^9}}],


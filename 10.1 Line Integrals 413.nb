Cell["First the parameterization, which should be easy.", "Text",
 CellChangeTimes->{3.676654034390397*^9}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"r", "[", "t_", "]"}], "=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"2", " ", 
     RowBox[{"Cos", "[", "t", "]"}]}], ",", 
    RowBox[{"2", 
     RowBox[{"Sin", "[", "t", "]"}]}]}], "}"}]}]], "Input",
 CellChangeTimes->{{3.676653959362124*^9, 3.676654002348256*^9}, 
   3.676654145480021*^9}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"2", " ", 
    RowBox[{"Cos", "[", "t", "]"}]}], ",", 
   RowBox[{"2", " ", 
    RowBox[{"Sin", "[", "t", "]"}]}]}], "}"}]], "Output",
 CellChangeTimes->{3.676654007988634*^9, 3.676654146891671*^9},
 Background->RGBColor[0.88, 1, 0.88]]
}, Open  ]],

Cell[TextData[{
 "Above: ",
 StyleBox["t",
  FontSlant->"Italic"],
 " will run from 0 to ",
 Cell[BoxData[
  FormBox[
   FractionBox["\[Pi]", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ". Now to define the vector field:"
}], "Text",
 CellChangeTimes->{{3.676654103065773*^9, 3.676654112976368*^9}, {
  3.676654265578938*^9, 3.676654273922832*^9}, {3.676654707316997*^9, 
  3.6766547151686087`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"ff", "[", 
   RowBox[{"{", 
    RowBox[{"x_", ",", "y_"}], "}"}], "]"}], "=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"x", " ", "y"}], ",", 
    RowBox[{
     SuperscriptBox["x", "2"], " ", 
     SuperscriptBox["y", "2"]}]}], "}"}]}]], "Input",
 CellChangeTimes->{{3.676654127619775*^9, 3.676654170974209*^9}, {
  3.676654221844761*^9, 3.6766542431957827`*^9}, {3.676654365521451*^9, 
  3.6766543686716957`*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"x", " ", "y"}], ",", 
   RowBox[{
    SuperscriptBox["x", "2"], " ", 
    SuperscriptBox["y", "2"]}]}], "}"}]], "Output",
 CellChangeTimes->{3.676654244858494*^9, 3.676654369916031*^9}]
}, Open  ]],

Cell["Below: then evaluate the field along the path:", "Text",
 CellChangeTimes->{{3.676654299757427*^9, 3.676654317241205*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"e1", "=", 
  RowBox[{"ff", "[", 
   RowBox[{"r", "[", "t", "]"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.676654323787911*^9, 3.676654333378091*^9}, 
   3.6766543907266827`*^9}],

Cell[BoxData[

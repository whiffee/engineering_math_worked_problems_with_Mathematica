  RowBox[{
   SubscriptBox[
    OverscriptBox["b", "~"], 
    RowBox[{"j", " ", "k"}]], "=", 
   TagBox[GridBox[{
      {"\[Piecewise]", GridBox[{
         {
          RowBox[{"-", "1"}], 
          RowBox[{
           RowBox[{"if", " ", "edge", " ", 
            SubscriptBox["e", "k"], " ", "leaves", " ", "vertex", " ", "j"}], 
           ","}]},
         {"1", 
          RowBox[{
           RowBox[{"if", " ", "edge", " ", 

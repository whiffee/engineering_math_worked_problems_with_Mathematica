Cell[TextData[{
 "11.  ODE. Find a solution of ",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["a", "2"], "-", 
        SuperscriptBox["x", "2"]}], ")"}], 
      RowBox[{"y", "''"}]}], "-", 
     RowBox[{"2", "x", " ", 
      RowBox[{"y", "'"}]}], "+", 
     RowBox[{"n", 
      RowBox[{"(", 
       RowBox[{"n", "+", "1"}], ")"}], "y"}]}], "=", "0"}], ",", " ", 
   RowBox[{"a", "\[NotEqual]", "0"}], ","}]]],
 " by reduction to the Legendre equation."
}], "Text",
 CellChangeTimes->{{3.768431001979042*^9, 3.768431050465609*^9}, {
  3.768431108296022*^9, 3.7684311200957603`*^9}},

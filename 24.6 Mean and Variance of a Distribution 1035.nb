Cell["I find the mean.", "Text",
 CellChangeTimes->{{3.766272510109789*^9, 3.7662725134056187`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"mu", "=", 
  RowBox[{"NIntegrate", "[", 
   RowBox[{
    RowBox[{"x", " ", 
     RowBox[{"f", "[", "x", "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", "0", ",", "1"}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.7662720087822447`*^9, 3.7662720116607437`*^9}, {
  3.76627209175596*^9, 3.7662721071301947`*^9}}],

Cell[BoxData["0.5000000000000002`"], "Output",
 CellChangeTimes->{3.7662720123876133`*^9, 3.766272107961918*^9},
 Background->RGBColor[0.88, 1, 0.88]]
}, Open  ]],

Cell["I find the variance.", "Text",
 CellChangeTimes->{{3.766272516467208*^9, 3.7662725492287197`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"sigsquaredpr", "=", 
  RowBox[{"NIntegrate", "[", 
   RowBox[{
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"x", "-", "mu"}], ")"}], "2"], 
     RowBox[{"f", "[", "x", "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", "0", ",", "1"}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.7662721604259377`*^9, 3.766272168240169*^9}}],

Cell[BoxData["0.05000000000000005`"], "Output",
 CellChangeTimes->{3.76627216924055*^9},
 Background->RGBColor[0.88, 1, 0.88]]
}, Open  ]],

Cell["\<\
According to numbered line (6) on p. 1037, the standardized random variable Z \
is given by\
\>", "Text",
 CellChangeTimes->{{3.766272287573452*^9, 3.7662723209635963`*^9}}],

Cell[BoxData[
 RowBox[{"Z", "=", 
  FractionBox[
   RowBox[{"X", "-", "\[Mu]"}], "\[Sigma]"]}]], "Text",
 CellChangeTimes->{{3.7662723261067657`*^9, 3.766272350693718*^9}}],

Cell["I already have everything, so just plugging in", "Text",

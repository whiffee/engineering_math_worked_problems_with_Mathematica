Cell["\<\
The green cell above matches the answer in the text, for series form. As for \
the radius of convergence, also using example 7,\
\>", "Text",
 CellChangeTimes->{{3.759606668824473*^9, 3.759606729997493*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Reduce", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"Abs", "[", 
     FractionBox[
      RowBox[{"z", "-", "\[ImaginaryI]"}], 
      RowBox[{"1", "-", "\[ImaginaryI]"}]], "]"}], "<", "1"}], ",", "z"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.759603815092149*^9, 3.75960388949212*^9}, {
  3.7596049254304523`*^9, 3.759604938155677*^9}, {3.7596050117140923`*^9, 
  3.759605017080063*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"-", 
    SqrtBox["2"]}], "<", 
   RowBox[{"Re", "[", "z", "]"}], "<", 
   SqrtBox["2"]}], "&&", 
  RowBox[{
   RowBox[{"1", "-", 
    FractionBox[
     SqrtBox[
      RowBox[{"4", "-", 
       RowBox[{"2", " ", 
        SuperscriptBox[
         RowBox[{"Re", "[", "z", "]"}], "2"]}]}]], 
     SqrtBox["2"]]}], "<", 
   RowBox[{"Im", "[", "z", "]"}], "<", 
   RowBox[{"1", "+", 
    FractionBox[
     SqrtBox[
      RowBox[{"4", "-", 
       RowBox[{"2", " ", 
        SuperscriptBox[
         RowBox[{"Re", "[", "z", "]"}], "2"]}]}]], 
     SqrtBox["2"]]}]}]}]], "Output",
 CellChangeTimes->{{3.759603868323016*^9, 3.759603890540172*^9}, {
   3.7596049325467243`*^9, 3.759604939317534*^9}, 3.75960501780796*^9},
 Background->RGBColor[1, 1, 0.85]]
}, Open  ]],


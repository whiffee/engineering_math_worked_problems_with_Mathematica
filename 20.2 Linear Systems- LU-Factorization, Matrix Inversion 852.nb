Cell[TextData[{
 "Since this subsection of problems was supposed to utilize Doolittle and not \
LUD, I will insert a Doolittle module and credit it to 2012rcampion as \
responder to the question on SEMma, ",
 StyleBox["https://mathematica.stackexchange.com/questions/78700/how-to-speed-\
up-auxilary-doolittledecomposite-function. ",
  FontSize->14,
  FontSlant->"Italic"],
 "It seems to have the same functionality as LUD, except that it does not \
include pivoting and condition information."
}], "Text",
 CellChangeTimes->{{3.764020534345221*^9, 3.764020622336557*^9}, {
   3.764020676380784*^9, 3.764020685811348*^9}, {3.764020879548265*^9, 
   3.764020913586795*^9}, {3.764020988562016*^9, 3.764021004423938*^9}, 
   3.7640213759335213`*^9, {3.764032662745277*^9, 3.764032665334374*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"doolittleDecomposite2", "[", 
   RowBox[{"mat_", "?", "MatrixQ"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"temp", "=", 
       RowBox[{"ConstantArray", "[", 
        RowBox[{"0", ",", 
         RowBox[{"Dimensions", "@", "mat"}]}], "]"}]}], ",", 
      RowBox[{"row", "=", 
       RowBox[{"Length", "@", "mat"}]}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Do", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"temp", "[", 
            RowBox[{"[", 
             RowBox[{"k", ",", "j"}], "]"}], "]"}], "=", 
           RowBox[{
            RowBox[{"mat", "[", 
             RowBox[{"[", 
              RowBox[{"k", ",", "j"}], "]"}], "]"}], "-", 
            RowBox[{
             RowBox[{"temp", "[", 
              RowBox[{"[", 
               RowBox[{"k", ",", " ", 
                RowBox[{";;", 
                 RowBox[{"k", "-", "1"}]}]}], "]"}], "]"}], ".", 
             RowBox[{"temp", "[", 
              RowBox[{"[", 
               RowBox[{
                RowBox[{";;", 
                 RowBox[{"k", "-", "1"}]}], ",", "j"}], "]"}], "]"}]}]}]}], 
          ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "k", ",", "row"}], "}"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Do", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"temp", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "k"}], "]"}], "]"}], "=", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"mat", "[", 
               RowBox[{"[", 
                RowBox[{"i", ",", "k"}], "]"}], "]"}], "-", 
              RowBox[{
               RowBox[{"temp", "[", 
                RowBox[{"[", 
                 RowBox[{"i", ",", " ", 
                  RowBox[{";;", 
                   RowBox[{"k", "-", "1"}]}]}], "]"}], "]"}], ".", 
               RowBox[{"temp", "[", 
                RowBox[{"[", 
                 RowBox[{
                  RowBox[{";;", 
                   RowBox[{"k", "-", "1"}]}], ",", "k"}], "]"}], "]"}]}]}], 
             ")"}], "/", 
            RowBox[{"temp", "[", 
             RowBox[{"[", 
              RowBox[{"k", ",", "k"}], "]"}], "]"}]}]}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", 
            RowBox[{"k", "+", "1"}], ",", "row"}], "}"}]}], "]"}], ";"}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "row"}], "}"}]}], "]"}], ";", "\[IndentingNewLine]",
      "temp"}]}], "]"}]}]], "Input"],


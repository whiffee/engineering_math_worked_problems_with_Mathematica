Cell[BoxData[
 RowBox[{
  RowBox[{"doolittleDecomposite2", "[", 
   RowBox[{"mat_", "?", "MatrixQ"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"temp", "=", 
       RowBox[{"ConstantArray", "[", 
        RowBox[{"0", ",", 
         RowBox[{"Dimensions", "@", "mat"}]}], "]"}]}], ",", 
      RowBox[{"row", "=", 
       RowBox[{"Length", "@", "mat"}]}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Do", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"temp", "[", 
            RowBox[{"[", 
             RowBox[{"k", ",", "j"}], "]"}], "]"}], "=", 
           RowBox[{
            RowBox[{"mat", "[", 
             RowBox[{"[", 
              RowBox[{"k", ",", "j"}], "]"}], "]"}], "-", 
            RowBox[{
             RowBox[{"temp", "[", 
              RowBox[{"[", 
               RowBox[{"k", ",", " ", 
                RowBox[{";;", 
                 RowBox[{"k", "-", "1"}]}]}], "]"}], "]"}], ".", 
             RowBox[{"temp", "[", 
              RowBox[{"[", 
               RowBox[{
                RowBox[{";;", 
                 RowBox[{"k", "-", "1"}]}], ",", "j"}], "]"}], "]"}]}]}]}], 
          ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "k", ",", "row"}], "}"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Do", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"temp", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "k"}], "]"}], "]"}], "=", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"mat", "[", 
               RowBox[{"[", 
                RowBox[{"i", ",", "k"}], "]"}], "]"}], "-", 
              RowBox[{
               RowBox[{"temp", "[", 
                RowBox[{"[", 
                 RowBox[{"i", ",", " ", 
                  RowBox[{";;", 
                   RowBox[{"k", "-", "1"}]}]}], "]"}], "]"}], ".", 
               RowBox[{"temp", "[", 
                RowBox[{"[", 
                 RowBox[{
                  RowBox[{";;", 
                   RowBox[{"k", "-", "1"}]}], ",", "k"}], "]"}], "]"}]}]}], 
             ")"}], "/", 
            RowBox[{"temp", "[", 
             RowBox[{"[", 
              RowBox[{"k", ",", "k"}], "]"}], "]"}]}]}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", 
            RowBox[{"k", "+", "1"}], ",", "row"}], "}"}]}], "]"}], ";"}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "row"}], "}"}]}], "]"}], ";", "\[IndentingNewLine]",
      "temp"}]}], "]"}]}]], "Input"],


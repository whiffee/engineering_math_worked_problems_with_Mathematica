Cell[BoxData[
 RowBox[{
  RowBox[{"GaussSeidelMat", "[", 
   RowBox[{
    RowBox[{"a_", "?", "MatrixQ"}], ",", " ", 
    RowBox[{"b_", "?", "MatrixQ"}], ",", 
    RowBox[{"x0_", "?", "MatrixQ"}], ",", "error_Real", ",", 
    "steps_Integer"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"l", ",", "u", ",", "x", ",", "abs"}], "}"}], ",", 
    RowBox[{
     RowBox[{
      RowBox[{"x", "[", "0", "]"}], "=", "x0"}], ";", 
     RowBox[{"l", "=", 
      RowBox[{"a", " ", 
       RowBox[{"SparseArray", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"{", 
            RowBox[{"i_", ",", "j_"}], "}"}], "/;", 
           RowBox[{"j", "\[LessEqual]", "i"}]}], "\[Rule]", "1"}], ",", 
         RowBox[{"{", 
          RowBox[{"3", ",", "3"}], "}"}]}], "]"}]}]}], ";", 
     RowBox[{"u", "=", 
      RowBox[{"a", " ", 
       RowBox[{"SparseArray", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"{", 
            RowBox[{"i_", ",", "j_"}], "}"}], "/;", 
           RowBox[{"j", ">", "i"}]}], "\[Rule]", "1"}], ",", 
         RowBox[{"{", 
          RowBox[{"3", ",", "3"}], "}"}]}], "]"}]}]}], ";", 
     RowBox[{
      RowBox[{"Reap", "[", 
       RowBox[{"Do", "[", " ", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"x", "[", "i", "]"}], "=", 
           RowBox[{
            RowBox[{"Inverse", "[", "l", "]"}], ".", 
            RowBox[{"(", 
             RowBox[{"b", "-", 
              RowBox[{"u", ".", 
               RowBox[{"x", "[", 
                RowBox[{"i", "-", "1"}], "]"}]}]}], ")"}]}]}], ";", 
          RowBox[{"abs", "=", 
           RowBox[{
            RowBox[{"Norm", "[", 
             RowBox[{
              RowBox[{"x", "[", "i", "]"}], "-", 
              RowBox[{"x", "[", 
               RowBox[{"i", "-", "1"}], "]"}]}], "]"}], "/", 
            RowBox[{"Norm", "[", 
             RowBox[{"x", "[", "i", "]"}], "]"}]}]}], ";", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"abs", "<", "error"}], ",", 
            RowBox[{
             RowBox[{"Sow", "@", 
              RowBox[{"x", "[", "i", "]"}]}], ";", 
             RowBox[{"Break", "[", "]"}]}]}], "]"}], ";", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"i", "\[Equal]", "steps"}], ",", 
            RowBox[{"Sow", "@", 
             RowBox[{"x", "[", "steps", "]"}]}]}], "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "steps"}], "}"}]}], "]"}], "]"}], "[", 
      RowBox[{"[", 
       RowBox[{
        RowBox[{"-", "1"}], ",", 
        RowBox[{"-", "1"}], ",", "1"}], "]"}], "]"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.7640761278887043`*^9, 3.7640765189601097`*^9}, {

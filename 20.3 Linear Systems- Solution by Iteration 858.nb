Cell[TextData[{
 "I got the Jacobi iteration code from ",
 StyleBox["https://community.wolfram.com/groups/-/m/t/831921",
  FontSize->14,
  FontSlant->"Italic"],
 ". And I haven\[CloseCurlyQuote]t analyzed it to make sure it is genuinely \
Jacobi algorithmic. But it seems to work. It has the slightly primitive \
quality of necessitating the setting of the variable k, in the ",
 StyleBox["While", "Input"],
 " loop, to fit each circumstance."
}], "Text",
 CellChangeTimes->{{3.764091441992124*^9, 3.76409147379943*^9}, {
  3.764091508285108*^9, 3.764091570835458*^9}, {3.764091642628254*^9, 
  3.76409172669484*^9}, {3.7640922391680517`*^9, 3.764092242848123*^9}}],

Cell[BoxData[
 RowBox[{"Clear", "[", "\"\<Global`*\>\"", "]"}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"jac", "[", 
    RowBox[{"A_", ",", "b_", ",", "xstart_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"n", ",", "k", ",", "i", ",", "j", ",", "xneu", ",", "xalt"}], 
      "}"}], ",", 
     RowBox[{
      RowBox[{"n", "=", 
       RowBox[{"Length", "[", "b", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"xalt", "=", "xstart"}], ";", "\[IndentingNewLine]", 
      RowBox[{"k", "=", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"k", "<", "9"}], ",", 
        RowBox[{
         RowBox[{"xneu", "=", "b"}], ";", "\[IndentingNewLine]", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"i", "=", "1"}], ",", 
           RowBox[{"i", "\[LessEqual]", "n"}], ",", 
           RowBox[{"i", "++"}], ",", 
           RowBox[{
            RowBox[{"For", "[", 
             RowBox[{
              RowBox[{"j", "=", "1"}], ",", 
              RowBox[{"j", "\[LessEqual]", "n"}], ",", 
              RowBox[{"j", "++"}], ",", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{"i", "\[NotEqual]", "j"}], ",", 
                RowBox[{
                 RowBox[{"xneu", "[", 
                  RowBox[{"[", "i", "]"}], "]"}], "=", 
                 RowBox[{
                  RowBox[{"xneu", "[", 
                   RowBox[{"[", "i", "]"}], "]"}], "-", 
                  RowBox[{
                   RowBox[{"A", "[", 
                    RowBox[{"[", 
                    RowBox[{"i", ",", "j"}], "]"}], "]"}], "*", 
                   RowBox[{"xalt", "[", 
                    RowBox[{"[", "j", "]"}], "]"}]}]}]}]}], "]"}]}], "]"}], 
            ";", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"xneu", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "=", 
             RowBox[{
              RowBox[{"xneu", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "/", 
              RowBox[{"A", "[", 
               RowBox[{"[", 
                RowBox[{"i", ",", "i"}], "]"}], "]"}]}]}]}]}], "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"xalt", "=", "xneu"}], ";", "\[IndentingNewLine]", 
         RowBox[{"k", "++"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
      "xalt"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.764091747839238*^9, 3.764091748382929*^9}, {
  3.764092000985615*^9, 3.764092017751116*^9}, {3.76409204917452*^9, 

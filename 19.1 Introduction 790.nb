Cell[BoxData[
 RowBox[{
  RowBox[{"Abs", "[", "\[Epsilon]", "]"}], "=", 
  RowBox[{
   RowBox[{"Abs", "[", 
    RowBox[{"x", "+", "y", "-", 
     RowBox[{"(", 
      RowBox[{
       OverscriptBox["x", "~"], "+", 
       OverscriptBox["y", "~"]}], ")"}]}], "]"}], "=", 
   RowBox[{
    RowBox[{"Abs", "[", 
     RowBox[{"x", "-", 
      OverscriptBox["x", "~"], "+", "y", "-", 
      OverscriptBox["y", "~"]}], "]"}], "=", 
    RowBox[{
     RowBox[{"Abs", "[", 
      RowBox[{
       SubscriptBox["\[Epsilon]", "x"], "+", 
       SubscriptBox["\[Epsilon]", "y"]}], "]"}], "\[LessEqual]", 
     RowBox[{
      RowBox[{"Abs", "[", 
       SubscriptBox["\[Epsilon]", "x"], "]"}], "+", 
      RowBox[{"Abs", "[", 
       SubscriptBox["\[Epsilon]", "y"], "]"}]}], "\[LessEqual]", 
     RowBox[{
      SubscriptBox["\[Beta]", "x"], "+", 

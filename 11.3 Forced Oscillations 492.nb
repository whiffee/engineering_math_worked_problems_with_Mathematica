Cell["\<\
The plot (above) does not look quite as expected. I feel I should emphasize \
that the described solution method is largely speculation.\
\>", "Text",
 CellChangeTimes->{{3.753825621280417*^9, 3.753825636134851*^9}, {
  3.7538260913382673`*^9, 3.753826093751615*^9}, {3.753826951005618*^9, 
  3.753826983282445*^9}}],

Cell[TextData[{
 "15. r(t) =  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "(", 
    RowBox[{
     SuperscriptBox["\[Pi]", "2"], "-", 
     SuperscriptBox["t", "2"]}], ")"}], TraditionalForm]]],
 "  if -\[Pi]<t<\[Pi] , and r(t+2\[Pi]) = r(t)"
}], "Text",
 CellChangeTimes->{{3.753580752348393*^9, 3.753580829340016*^9}, {
  3.753729362047821*^9, 3.7537294127965612`*^9}, {3.7537294690438538`*^9, 
  3.7537294702137613`*^9}, {3.7537295507924547`*^9, 3.7537296666679153`*^9}},
 Background->RGBColor[1, 0.9, 0.8]],

Cell["\<\
This problem is covered in the s.m.. The observation, made there and visible \
from the problem description, is that the function r[t] is odd and that the \
function\[CloseCurlyQuote]s cycle is 2\[Pi]. At this point I check the \
Fourier series.\
\>", "Text",
 CellChangeTimes->{{3.75372970681879*^9, 3.753729765176668*^9}, {
  3.753729819288184*^9, 3.753729833694367*^9}, {3.753813726781335*^9, 
  3.753813749145711*^9}, {3.753816469608718*^9, 3.753816477839136*^9}, {
  3.7702948306297083`*^9, 3.770294846539316*^9}}],

Cell[BoxData[
 RowBox[{"Clear", "[", "\"\<Global`*\>\"", "]"}]], "Input",
 CellChangeTimes->{{3.753729847289774*^9, 3.7537298661843243`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"eq1", "=", 
  RowBox[{"FourierSeries", "[", 
   RowBox[{
    RowBox[{"t", 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["\[Pi]", "2"], "-", 
       SuperscriptBox["t", "2"]}], ")"}]}], ",", "t", ",", "1"}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.753729874228457*^9, 3.753729919275647*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"6", " ", "\[ImaginaryI]", " ", 
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{
     RowBox[{"-", "\[ImaginaryI]"}], " ", "t"}]]}], "-", 
  RowBox[{"6", " ", "\[ImaginaryI]", " ", 
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{"\[ImaginaryI]", " ", "t"}]]}]}]], "Output",
 CellChangeTimes->{3.753730810463788*^9, 3.753733337019589*^9, 
  3.753795529222994*^9, 3.753800210209875*^9, 3.753804026386675*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"eq2", " ", "=", 
  RowBox[{"ExpToTrig", "[", 
   RowBox[{
    RowBox[{"6", " ", "\[ImaginaryI]", " ", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "\[ImaginaryI]"}], " ", "t"}]]}], "-", 
    RowBox[{"6", " ", "\[ImaginaryI]", " ", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{"\[ImaginaryI]", " ", "t"}]]}]}], "]"}]}]], "Input",

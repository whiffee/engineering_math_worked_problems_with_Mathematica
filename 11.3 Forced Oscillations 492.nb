        SuperscriptBox["n", "2"]}], 
       RowBox[{"1", "-", 
        RowBox[{"2", " ", 
         SuperscriptBox["n", "2"]}], "+", 
        RowBox[{
         SuperscriptBox["c", "2"], " ", 
         SuperscriptBox["n", "2"]}], "+", 
        SuperscriptBox["n", "4"]}]]}]}], ",", 
    RowBox[{"b", "\[Rule]", 
